<!doctype html>
<html lang="en">
<head>
<title>Welcome!</title>
  <meta charset="utf-8">
  <link rel="stylesheet" href="http://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">
  <link href="/css/signup.css" rel="stylesheet" type="text/css">
  <link href="/css/nav.css" rel="stylesheet" type="text/css">
  <script src="/js/signup.js"></script>

</head>
<% include partials/headerlog.ejs %>
<body>

<div class="container">  
    <div id="app" class="col-md-2">
      <form action="/login" method="post">       
        <div class="form-group">
          <label for="name">Name:</label>
          <input type="text" class="form-control" required="true" maxlength="10" id="name" name="name">
        </div>
        <div class="form-group">
          <label for="pwd">Password:</label>
          <input type="password" class="form-control" required="true" maxlength="20" id="pwd" name="password">
        </div>
        <h2>Signup Please</h2>
        <button type="submit" class="btn-danger center-block">Submit</button>

      </form>
    </div>  
</div>

</body>
</html>